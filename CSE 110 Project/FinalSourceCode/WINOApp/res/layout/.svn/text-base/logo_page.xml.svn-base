<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/cream_texture" >

    <ImageView
        android:id="@+id/blank_logo_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:src="@drawable/wino_rhino_transparent" />

    <TextView
        android:id="@+id/blank_logo_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/blank_logo_logo"
        android:layout_centerHorizontal="true"
        android:textIsSelectable="true" 
        android:textSize="20dp"
        android:textColor="#801108" />

</RelativeLayout>